<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="dog" class="com.yw.test01.Dog">
		<constructor-arg type="boolean" value="true">
		</constructor-arg>

		<property name="a" value="10" />
	</bean>

	<bean id="dog2" class="com.yw.test01.Dog">
		<constructor-arg type="Integer" value="3">
		</constructor-arg>
	</bean>
	
	<bean id="dog3" class="com.yw.test01.Dog">
		<property name="c" value="hello"></property>
	</bean>
</beans>